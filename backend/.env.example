# PJ18 Apple Backend - Environment Variables Example
# Copy this file to .env and fill in your API keys

FRONTEND_URL=http://localhost:3000
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/pj18_apple

# 공공데이터포털 API 키 (기상청) - https://www.data.go.kr/
DATA_PORTAL_API_KEY=your_api_key_here

# KAMIS 농산물유통정보 API - https://www.kamis.or.kr/
KAMIS_API_KEY=your_api_key_here
KAMIS_API_ID=your_api_id_here

# 브이월드 API - https://www.vworld.kr/
VWORLD_API_KEY=your_api_key_here

# 통계청 KOSIS API - https://kosis.kr/openapi/
KOSIS_API_KEY=your_api_key_here

# OpenAI API (시뮬레이션 검증용, 선택)
OPENAI_API_KEY=your_openai_api_key_here
